    <script>
    let currentStep = 0;
    function setStep(i) {
    	currentStep = i;
    	const step = i-1;
    	$('.u-section-header .u-layout-cell').removeClass('active');
    	$('.u-section-header .u-layout-cell').each((idx,obj)=>{
    		if(idx == (step*2) || idx == (step*2+1))
    		$(obj).addClass('active');
    	});
    }
    function clickPrev() {
    	let url = "";
    	switch(currentStep) {
    	case 2:
    		url = "/reservation/step/selectMachine";
    		break;
    	case 3:
    		url = "/reservation/step/selectCourse";
    		break;
    	case 4:
    		url = "/reservation/step/selectEquipment";
    		break;
    	default :
    		return;
    	}
    	location.href = url;
    }
    </script>